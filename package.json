  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-ts",
    "build-ts": "npx tsc",
    "watch-ts": "tsc -w",
    "eslint": "eslint 'src/**'",
    "eslint-fix": "eslint 'src/**' --fix",
    "prod-start": "NODE_ENV=prod npm run build && npm start",
    "dev-start": "NODE_ENV=test npm run build && npm start",
    "start": "NODE_ENV=prod npm run build && node dist/index.js",
    "update-deps": "git submodule init && git submodule update --remote --recursive",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ]
  },
  "bin": {
    "ssa-bot": "./dist/cli.js"
  },
  "author": "Keyko",
  "license": "ISC",
  "dependencies": {
    "@keyko-io/filecoin-content": "file:deps/filecoin-content",
    "@keyko-io/filecoin-verifier-tools": "2.1.2",
    "@octokit/auth-app": "^3.6.0",
    "@octokit/core": "^3.6.0",
    "@octokit/rest": "^18.12.0",
    "@wtfcode/byte-converter": "^1.7.11",
    "aws-sdk": "^2.1450.0",
    "axios": "^0.21.4",
    "commander": "^11.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "filecoin-verfier-common": "^1.0.7",
    "node-cron": "^3.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@octokit/types": "^6.35.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.5",
    "@types/node": "^16.7.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "jest-expect-message": "^1.1.3",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^4.3.5"
  }
